{
    "schema": "https://openapi.vercel.sh/vercel.json",
    "version": 2,
    "builds": [
        {
            "src": "api/index.cjs",
            "use": "@vercel/node",
            "buildCommmand": "npm run build:vercel"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/dist/api/$1"
        }
    ]
}
